pipeline{
    agent any
    environment{
    PATH = "/bin/mvn:$PATH" 
    }
    stages{
      stage("Git Checkout"){
         steps{
         git credentialsId: 'AnkitGithub', url: 'https://github.com/javahometech/myweb'
        }
     }
      
      stage("Maven Build"){
         steps{
         sh "mvn clean package"
        }
     }
  }
}
